    "vue-property-decorator": "^9.1.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "cz-conventional-changelog": "^3.3.0",
    "prettier": "^2.2.1",
    "typescript": "~4.1.5",
    "vue-template-compiler": "^2.6.11"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wenye123/vue-swipe.git"
  },
  "keywords": [
    "typescript",
    "vue-swipe",
    "vue2",
    "vue",
    "swipe"
  ],
  "author": "wenye <wenye2333@gmail.com>",
  "bugs": {
    "url": "https://github.com/wenye123/vue-swipe/issues"
  },
  "homepage": "https://github.com/wenye123/vue-swipe",
