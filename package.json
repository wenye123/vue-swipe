{
  "name": "@wenye123/vue-swipe",
  "version": "0.1.2",
  "license": "MIT",
  "description": "一个基于vue2.0的swipe库",
  "main": "dist/swipe.js",
  "files": [
    "dist/swipe.css",
    "dist/swipe.js"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
